<div id="gs-group-messages-posts">
  <tal:block condition="view/viewTopics">
    <p id="gs-group-messages-posts-loading" style="display:none;">
      <img style="margin:0px 0.69em 0px 0px;padding:0px;"
        src="/++resource++anim/wait.gif"/> 
      Loading the posts
    </p><!--gs-group-messages-posts-loading-->
    <div id="gs-group-messages-posts-latest" 
      style="display:none;">
      &#160;
    </div><!--gs-group-messages-posts-latest-->
    <div id="gs-group-messages-posts-toolbar">
      <span class="ui-widget-header ui-corner-all"
        style="padding: 0.692em;">
        <button id="gs-group-messages-posts-toolbar-prev">Newer</button>
        <button id="gs-group-messages-posts-toolbar-next">Older</button>
      </span>
    </div><!--gs-group-messages-posts-toolbar-->
    <div id="gs-group-messages-posts-tasks">
      <ul class="inline-list">
        <li><a href="messages/posts.html">The <strong>posts</strong> 
            page</a></li>
        <li><a type="application/atom+xml" class="alternateATOM"
          tal:define="title string:Posts in ${view/groupInfo/name};
            url string:/s/search.atom?t=0&amp;f=0&amp;p=1&amp;g=${view/groupInfo/id}"
          tal:attributes="href url" href="#">Web feed</a></li>

      </ul>
    </div><!--gs-group-messages-posts-tasks-->
  </tal:block>
</div><!--gs-group-messages-posts-->
